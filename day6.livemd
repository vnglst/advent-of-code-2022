defmodule Unique do
  def unique?(list) do
    unique_size =
      list
      |> Enum.uniq()
      |> Enum.count()

    unique_size == Enum.count(list)
  end
end

""
|> String.graphemes()
|> Enum.reduce_while({[], 0}, fn char, acc ->
  {str, idx} = acc
  str = ([char] ++ str) |> Enum.take(14)

  cond do
    Enum.count(str) < 4 -> {:cont, {str, idx + 1}}
    Unique.unique?(str) -> {:halt, {Enum.reverse(str), idx + 1}}
    true -> {:cont, {str, idx + 1}}
  end
end)
